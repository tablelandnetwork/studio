{
  "name": "@tableland/studio-mail",
  "version": "0.0.1",
  "repository": "https://github.com/tablelandnetwork/studio/mail",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier '**/*.{ts,json}' --check",
    "prettier:fix": "npm run prettier -- --write",
    "dev": "email dev --dir src/emails",
    "export": "email export --dir src/emails",
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "license": "MIT AND Apache-2.0",
  "dependencies": {
    "@react-email/components": "0.0.6",
    "@react-email/render": "^0.0.12",
    "postmark": "^3.0.19",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "1.9.3"
  }
}
