{
  "name": "@tableland/studio-validators",
  "version": "0.0.0-pre.2",
  "description": "Shared Zod schemas for Studio packages",
  "repository": "https://github.com/tablelandnetwork/studio/validators",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.js",
  "files": [
    "dist/**/*.js"
  ],
  "scripts": {
