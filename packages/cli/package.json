{
  "name": "@tableland/studio-cli",
  "version": "0.2.0",
  "description": "Tableland command line tools",
  "repository": "https://github.com/tablelandnetwork/studio/cli",
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "ethereum-waffle": {
      "@nomicfoundation/hardhat-chai-matchers": "*"
    }
  },
  "keywords": [
    "tableland",
    "studio",
    "sql",
    "ethereum",
    "cli",
    "database"
  ],
  "type": "module",
  "main": "./dist/cli.js",
  "module": "./dist/cli.js",
  "bin": {
    "studio": "./dist/cli.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "test": "mocha",
    "coverage": "TEST_TIMEOUT_FACTOR=3 c8 --100 --exclude test mocha --exit",
    "coverage:report": "c8 report --reporter=html",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier '**/*.{ts,json}' --check",
    "prettier:fix": "npm run prettier -- --write",
    "clean": "rm -rf dist",
    "build": "tsc && chmod +x dist/cli.js && ./fixup",
    "studio": "node ./dist/cli.js"
  },
  "license": "MIT AND Apache-2.0",
  "dependencies": {
    "@tableland/sdk": "^7.1.0",
    "@tableland/sqlparser": "^1.3.0",
    "@tableland/studio-client": "0.0.2",
    "@tableland/studio-validators": "^0.0.2",
    "chalk": "^5.3.0",
    "cli-select-2": "^2.0.0",
    "cosmiconfig": "^8.0.0",
    "csv-parse": "^5.5.0",
    "dotenv": "^16.0.3",
    "ethers": "^6.12.1",
    "inquirer": "^9.1.2",
    "js-yaml": "^4.1.0",
    "keccak": "^3.0.1",
    "readline": "^1.3.0",
    "siwe": "^2.3.2",
    "table": "^6.8.1",
    "yargs": "^17.6.2",
    "zod": "^3.23.4"
  }
}
