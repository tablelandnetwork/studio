{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "declaration": true,
    "importHelpers": false,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noErrorTruncation": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
